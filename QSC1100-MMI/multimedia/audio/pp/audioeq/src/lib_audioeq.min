#-------------------------------------------------------------------------------
# Audio Equalizer Library
#-------------------------------------------------------------------------------

AUDIOEQ_LIB_SOURCES=\
	audioeq.c

AUDIOEQ_LIB_OBJS :=  $(addprefix $(LIBDIR)/, $(AUDIOEQ_LIB_SOURCES:%.c=%.o))       
vpath %.c $(AUDIOEQ)

$(LIBDIR)/audioeq.lib : $(AUDIOEQ_LIB_OBJS)
	@echo ------------------------------------------------------------------
	@echo LIBRARY $@
	@if test -f $@; then rm -f $@; fi
	$(ARMAR) -r $@ $(AUDIOEQ_LIB_OBJS)
	@echo ------------------------------------------------------------------
	       
LIBOBJECTS := $(AUDIOEQ_LIB_OBJS) $(LIBOBJECTS)
LIBRARIES := $(LIBRARIES) $(LIBDIR)/audioeq.lib

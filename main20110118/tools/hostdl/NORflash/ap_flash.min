#----------------------------------------------------------------------------
#   Does this target use NAND or not?
#----------------------------------------------------------------------------
USES_NAND = NO


#----------------------------------------------------------------------------
#   NOR flash driver objects
#----------------------------------------------------------------------------
NOR_DRIVER_OBJS=\
	$(TARGET)/nor_amd.o


#----------------------------------------------------------------------------
#   Other Flash objects for NOR flash
#----------------------------------------------------------------------------
NOR_FLASH_OBJS=$(TARGET)/ap_flash.o


FLASH_OBJS=	$(NOR_FLASH_OBJS) $(NOR_DRIVER_OBJS)

vpath %.c norflash


#----------------------------------------------------------------------------
#   Define any flags needed for this specific flash type to compile correclty
#----------------------------------------------------------------------------
FLASH_TYPE_CFLAGS = 

FLASHINC =-Inorflash



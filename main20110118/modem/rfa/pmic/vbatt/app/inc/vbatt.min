#-------------------------------------------------------------------------------
# VBATT (battery voltage drivers)
#-------------------------------------------------------------------------------

VBATT_SOURCES=\
    vbatt.c

VBATT_PMIC_SOURCES=\
    vbatt_task.c                \
    vbatt_pmic_comparator.c     

ifeq ($(USES_VBATT_PMIC), yes)
	VBATT_SOURCES += $(VBATT_PMIC_SOURCES)
endif


VBATT_OBJS :=  $(addprefix $(TARGETDIR)/, $(VBATT_SOURCES:%.c=%.o))
vpath %.c $(VBATT)


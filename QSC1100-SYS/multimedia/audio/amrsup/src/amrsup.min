#-------------------------------------------------------------------------------
# AMRSUP (AMR supplementary functions)
#-------------------------------------------------------------------------------

AMRSUP_SOURCES=\
        amrsup.c

vpath %.c $(AMRSUP)

ifeq ($(BIG),yes)
        AMRSUP_OBJS := $(TARGETDIR)/amrsup.lib
else
	AMRSUP_OBJS :=  $(addprefix $(TARGETDIR)/, $(AMRSUP_SOURCES:%.c=%.o))
endif

$(TARGETDIR)/amrsup.lib : $(AMRSUP_SOURCES)
	$(group_c_compile)

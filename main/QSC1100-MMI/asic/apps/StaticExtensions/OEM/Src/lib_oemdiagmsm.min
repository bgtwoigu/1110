#-------------------------------------------------------------------------------
# OEMDiagMSM Library
#-------------------------------------------------------------------------------

OEMDIAGMSM_LIB_SOURCES=\
	OEMDiagMSM.c

OEMDIAGMSM_LIB_OBJS :=  $(addprefix $(LIBDIR)/, $(OEMDIAGMSM_LIB_SOURCES:%.c=%.o))       
vpath %.c $(OEMDIAGMSM)

$(LIBDIR)/OEMDiagMSM.lib : $(OEMDIAGMSM_LIB_OBJS)
	@echo ------------------------------------------------------------------
	@echo LIBRARY $@
	@if test -f $@; then rm -f $@; fi
	$(ARMAR) -r $@ $(OEMDIAGMSM_LIB_OBJS)
	@echo ------------------------------------------------------------------
	       
LIBOBJECTS := $(OEMDIAGMSM_LIB_OBJS) $(LIBOBJECTS)
LIBRARIES := $(LIBRARIES) $(LIBDIR)/OEMDiagMSM.lib

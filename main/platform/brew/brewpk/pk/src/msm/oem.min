ifneq ($(BUIW_STATIC_LOADING),yes)
OEM_LIB_SOURCES +=  \
        xmodimpl.c
vpath %.c $(BUIW)/xmod
vpath %.c $(UIONESDK12)/Fonts/BREWTrueTypeFont/xmod
vpath %.c $(UIONESDK12)/Fonts/BREWBitmapFont/xmod
endif
endif


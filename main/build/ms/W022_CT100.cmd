@echo off

time.pl
call buildtime.bat
echo BUILD_TIME=%BUILD_TIME% MODEL_BUILD_TIME=%MODEL_BUILD_TIME%

@set PROJECTNAME=W022_CT100
@set BASEPROJECTNAME=W022
@set QUALCOMMNAME=SBNAAHZ
@set QUALCOMMNAME_BUILD=SXNAAHZ
@set RESOURCEDIR=CT100
@set MODELVERSION=W022_Z80C_SC_FM_V02_12864_%BUILD_TIME%
@set HWVERSION=W022_MB_V0.6
@set MODELBUILDTIME=%MODEL_BUILD_TIME%
@set SVNVERSION=0002


IF "%*"=="" goto DO_COPY
goto DO_WORK
:DO_COPY
xcopy %QUALCOMMNAME_BUILD% %PROJECTNAME% /S /Q /I /Y /D 2>&1 > temp.tmp
xcopy ..\..\core\boot\romboot\osbl\osbl_lib_%QUALCOMMNAME% ..\..\core\boot\romboot\osbl\osbl_lib_%PROJECTNAME% /S /Q /I /Y /D 2>&1 > temp.tmp
xcopy ..\..\core\boot\romboot\dbl\dbl_lib_%QUALCOMMNAME% ..\..\core\boot\romboot\dbl\dbl_lib_%PROJECTNAME% /S /Q /I /Y /D 2>&1 > temp.tmp
xcopy ..\..\core\boot\romboot\fsbl\fsbl_lib_%QUALCOMMNAME% ..\..\core\boot\romboot\fsbl\fsbl_lib_%PROJECTNAME% /S /Q /I /Y /D 2>&1 > temp.tmp
cd %PROJECTNAME%\libs\hsusb
IF NOT EXIST %PROJECTNAME% ren %QUALCOMMNAME% %PROJECTNAME%
cd ..\..\..

:DO_WORK
del /f /a build%PROJECTNAME%.log.old

if exist build%PROJECTNAME%.log move build%PROJECTNAME%.log build%PROJECTNAME%.log.old > nul

make -r -f dmss1100.mak %*  USES_REND=yes USES_RELEASE_VERSION=yes USES_CONSTEFS=yes USES_MMI=yes EXTLANG1=hi EXTLANG2=NO USES_IIPL=yes USES_LCD_SIZE=128x160 USES_RESDIR=%RESOURCEDIR% USES_CAMERA=yes USES_NO_DEBUG=yes USES_HSU_MS_FD=yes USES_HFAT=yes USES_SDCC=yes USES_SECBOOT_2_0=yes USES_SPANSION_S29WS256N0SB=yes USES_HS_USB_ON_MODEM_PROC=yes USES_BREW_3.1=yes USES_LINKER_XREFS=yes USES_HS_USB_PMIC_PHY=yes USES_XO=yes USES_NOR_PSRAM=yes USES_OSBL_SBI_PMIC_LIB=yes USES_AUDFMT_AMR=yes USES_IALARM=yes USES_CDMA=yes USES_NO_STRIP_NO_ODM=yes USES_RFMSM=yes USES_OEMSBL_SPLASH_SCREEN=yes USES_MDP=yes USES_MMOVERLAY=yes USES_ROMBOOT=yes USES_CHARGER=yes USES_BREW_SMS=yes USES_GENERATE_MDSP=yes USES_LOWTIER_LOWMEM=yes USES_FSBL_SBI_PMIC_LIB=yes USES_PROCESS_ERROR_CAL=yes USES_MEM_USAGE_HTML=yes USES_OSBL_CHG_SCREEN=no USES_CLEAN_REMOVE=yes USES_AUTH=yes USES_BREW_BUIW=no USES_QHSUSB_DEAD_BATTERY_CHARGING=yes USES_SURF=yes USES_ERR_EXCLUDE_RESET_DETECT=yes USES_SPLIT_CODE_DATA=yes USES_REMOVE_WMS_RESOURCES=yes USES_HSU=yes USES_SNDCTL=yes USES_SBI=yes USES_AUDIOEQ=yes USES_UIM=yes USES_BREW_APPMGR=no USES_NORPRG_HDR=yes USES_EFS2=yes USES_ZBLD=yes USES_HS_USB_CHG_REMOTE_API=yes USES_BOOT_SPLASH_SCREEN=yes USES_BUILD_FLASHTOOLS=yes USES_STRIP=yes USES_QSC1100_TARGET=no USES_RF_QSC1100=yes USES_ARM_SBC_CODEC=yes USES_LOW=yes USES_QHSUSB=yes USES_SERVICES_ERR=yes USES_MP4=yes USES_QTVVIDEO=yes USES_CAMCORDER=yes USES_QSC1110=yes USES_SEC=yes USES_SEC_CLNT=yes USES_SEC_SVC=yes USES_SFS=yes ASIC=q1100 BUILD=%PROJECTNAME% VERSION=3375 BUILDDIR=%PROJECTNAME% 2>&1 | ..\..\tools\build\qtee build%PROJECTNAME%.log

IF %ERRORLEVEL% == 0 goto SUCCESSMSG
IF NOT %ERRORLEVEL% == 0 goto FAILEDMSG
goto END

:FAILEDMSG
rem echo msgbox "编译出错，请修正！">msg.vbs
rem msg.vbs
rem del msg.vbs
msg %username% /TIME:60000 编译出错，请修正！
goto END

:SUCCESSMSG
rem echo msgbox "编译成功！">msg.vbs
rem msg.vbs
rem del msg.vbs
IF NOT "%*"=="" goto END
msg %username% /TIME:60000 编译完成！
goto END

:END
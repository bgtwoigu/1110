    dspMessageReg
    adpcmPadWord
    codecRxGain
    adpcmState
    adpcmVersion
    upDmaPathControl
    adecPauseFlagCh0
    adecDecoderTypeCh0
    pcmDmaMissed
    codecRxOffsetL
    codecRxOffsetR
  END SYMBOLS

  BEGIN ALIASES
    adecDataIsNeededCh0          = adecAdpcmDataIsNeeded0
    adecIsActiveCh0              = adecAdpcmActive0
    adecPauseFlagCh0             = adecPause
    hostStickyInterrupt          = adecDataHostStickyInt
    adecDataReadPtrOffsetCh0     = adecAdpcmDataReadPtrOffset0
    adecResetFlagCh0             = adecReset
    adpcmCommandBuf              = uPCommandBuf
    adpcmCommandReg              = uPCommandReg
    adpcmPadWord                 = xmitPadWord
    adpcmState                   = qdspState
    masterVolume                 = rxVolume	
    adpcmVersion                += ramVersion
    ramVersion + 4              += romVersion
    adecStereoPan                = masterPan
  END ALIASES
END SECTION // ADPCM
  

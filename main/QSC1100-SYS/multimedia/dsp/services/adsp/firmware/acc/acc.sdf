BEGIN SECTION ADPCM
  BEGIN SYMBOLS
    adecIsActiveCh0
    adecChannelModeCh0
    adecDataBufSize1Ch0
    adecDataBufStartMSW1Ch0
    adecDataBufSize2Ch0
    adecDataBufStartMSW2Ch0
    adecDataBufReadyFlag1Ch0
    adecDataBufReadyFlag2Ch0
    adecDataIsNeededCh0
    adecDataReadPtrOffsetCh0
    adecStopFlagCh0
    hostStickyInterrupt
    adecMVolume
    adecResetFlagCh0
    adecStereoPan
    masterVolume
    adpcmCommandBuf
    adpcmCommandReg
    adpcmDecoderType

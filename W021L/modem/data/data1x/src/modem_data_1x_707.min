### Start Lint block ###
ifeq ($(DO_LINT),yes)
  ## include the all the min files containing data code ## 
  include $(DATA)/modem_data_1x_707.min
  include $(DATA)/modem_data_1x_jcdma.min
  include $(DATA)/modem_data_1x_mip.min
  include $(DATA)/modem_data_1x_1xrlp.min
  ##  add all the source files of data ## 
  DATA_COMBINED_LINT_SOURCES := $(MODEM_DATA_1X_707_SOURCES) $(DATA_JCDMA_SOURCES) $(DATA_MIP_SOURCES) $(DATA_RLP_SOURCES)

  DATA_LINT_OBJS := $(addprefix $(LINTDIR)/, $(DATA_COMBINED_LINT_SOURCES:%.c=%.lint))

  ##  Specify group target for linting ## 
  $(LINTDIR)/data_all_lint : $(DATA_LINT_OBJS)
endif

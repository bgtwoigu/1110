EXT_QAUDIOFX_LIB_SOURCES = 

ifeq ($(USES_QAUDIOFX), yes)
  EXT_QAUDIOFX_LIB_SOURCES += OEMMediaAudio.c
endif

EXT_QAUDIOFX_LIB_OBJS :=  $(addprefix $(TARGETDIR)/, $(EXT_QAUDIOFX_LIB_SOURCES:%.c=%.o))

vpath %.c $(QAUDIOFXSTATICEXT)

EXT_QAUDIOFX_OBJS := $(EXT_QAUDIOFX_LIB_OBJS)
$(EXT_QAUDIOFX_OBJS): APPFLAGS = -DBREW_STATIC_APP -DAEE_STATIC
